#!/bin/sh
currTime=`date +%k%M`
check_time_to_run()
{
      tempTime=$1
          if [ $tempTime -gt 200 -a $tempTime -lt 2200 ]; then 
                    echo "Time is between 2 AM and 10 PM. Aborting."
                            exit 1
                                else
                                          echo "Time is after 10 PM and before 2 AM. Running normally."
                                              fi
                                            }

                                            check_time_to_run $currentTime
